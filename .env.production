# Configuration de production pour Railway
ENVIRONMENT=railway

# Base de données PostgreSQL Railway (sera remplacé par les variables d'environnement Railway)
PGHOST=${PGHOST}
PGPORT=${PGPORT}
PGDATABASE=${PGDATABASE}
PGUSER=${PGUSER}
PGPASSWORD=${PGPASSWORD}

# Configuration de l'application
APP_NAME=AppWoyofal
APP_ENV=production
APP_DEBUG=false
APP_URL=https://appwoyofal.railway.app

# Configuration des logs
LOG_LEVEL=error

# Configuration de sécurité
ALLOWED_HOSTS=appwoyofal.railway.app,*.railway.app

# Timezone
TIMEZONE=Africa/Dakar
